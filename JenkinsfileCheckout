pipeline {
    agent any

    stages {
        stage('Checkout') {
          steps {
          checkout([$class: 'GitSCM', branches: [[name: '*/featurebranch']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/durgasai/kuberneteslearning.git']]])
        }
        }
        stage('Display Contents') {
        steps {
          sh 'ls'
          }
        }
    }
}
